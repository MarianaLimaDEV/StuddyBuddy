{
  "env": {
    "browser": true,
    "es2022": true,
    "module": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "eqeqeq": ["error", "always"],
    "no-else-return": "warn",
    "no-multi-spaces": "warn",
    "no-trailing-spaces": "warn",
    "prefer-const": "warn",
    "no-var": "error"
  }
}

